{
  "name": "project-name",
  "version": "0.0.1",
  "description": "Description of your project",
  "scripts": {
    "dev": "gulp",
    "build": "gulp clean && gulp build"
  },
  "proxy": {
    "open": false,
    "path": "localhost/[your-project]/app"
  },
  "vars": {
    "bundleJS": "main.js",
    "bundleCSS": "style.css",
    "scss": "style.scss"
  },
  "autoPrefixBrowserList": [
    "last 2 version",
    "safari 5",
    "ie 8",
    "ie 9",
    "opera 12.1",
    "ios 6",
    "android 4"
  ],
  "paths": {
    "app": {
      "base": "./app",
      "img": "./app/images",
      "font": "./app/fonts",
      "js": "./app/script",
      "jsSrc": "./app/script/src",
      "jsVendor": "./app/script/vendors",
      "scss": "./app/style/scss",
      "css": "./app/style"
    },
    "output": {
      "base": "./dist",
      "img": "./dist/images",
      "font": "./dist/fonts",
      "js": "./dist/script",
      "css": "./dist/style"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-preset-es2015": "^6.18.0",
    "browser-sync": "^2.9.10",
    "critical": "^0.9.1",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-babel": "^6.1.2",
    "gulp-clean": "^0.3.2",
    "gulp-clean-css": "^3.4.0",
    "gulp-concat": "^2.6.0",
    "gulp-imagemin": "^3.3.0",
    "gulp-plumber": "^1.0.1",
    "gulp-sass": "^3.1.0",
    "gulp-sequence": "^0.4.1",
    "gulp-shell": "^0.6.3",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.6"
  }
}
